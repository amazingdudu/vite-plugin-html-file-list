{
    "name": "vite-plugin-html-file-list",
    "version": "1.0.0",
    "description": "Lists all html files under the current project, useful for multi-page applications",
    "keywords": [
        "vite",
        "vite-plugin",
        "html",
        "file list",
        "html file list"
    ],
    "homepage": "https://github.com/amazingdudu/vite-plugin-html-file-list#readme",
    "bugs": {
        "url": "https://github.com/amazingdudu/vite-plugin-html-file-list/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/amazingdudu/vite-plugin-html-file-list.git"
    },
    "license": "MIT",
    "author": "amazingdudu",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "npm run eslint && unbuild",
        "eslint": "eslint --ext .ts ./ --fix",
        "prepare": "husky install",
        "prettier": "prettier -c -w '**/*'",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "lint-staged": {
        "*.{ts}": "eslint --cache --fix",
        "*": "prettier -w"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.30.4",
        "@typescript-eslint/parser": "^5.30.4",
        "eslint": "^8.19.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-packagejson": "^2.2.18",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.7.4",
        "unbuild": "^0.8.9",
        "vite": "^3.0.9"
    },
    "peerDependencies": {
        "vite": "^3.0.9"
    },
    "engines": {
        "node": ">=14.18.0"
    }
}
